{
  "name": "react-speed-book",
  "version": "1.0.0",
  "description": "Companion code for React Speed Coding book",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "SET NODE_ENV=development & webpack-dev-server --inline",
    "build": "SET NODE_ENV=production & webpack --config webpack.prod.config.js",
    "profile": "SET NODE_ENV=production & webpack --config webpack.prod.config.js --profile --json > profile.json",
    "lint": "SET NODE_ENV=lint & webpack-dev-server --config webpack.lint.config.js",
    "test": "SET NODE_ENV=test & mocha --compilers js:babel-core/register --recursive || true",
    "cover": "SET NODE_ENV=test & babel-node ./node_modules/.bin/babel-istanbul cover _mocha --  --require test/browser.js test/**/*.spec.js --reporter dot || true",
    "storybook": "start-storybook -p 9001 -s ./app/public"
  },
  "author": "Sasha",
  "license": "MIT",
  "dependencies": {
    "firebase": "^3.6.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "rumble-charts": "^1.0.14"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.33.0",
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "browser-sync": "^2.17.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.13",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-minifier": "^3.1.0",
    "html-webpack-plugin": "^2.24.0",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "normalize.css": "^5.0.0",
    "postcss-easy-import": "^1.0.1",
    "postcss-loader": "^1.0.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "remote-redux-devtools": "^0.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
